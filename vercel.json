<yb_canvas uuid="123e4567-e89b-12d3-a456-426614174003" mversion="1" type="code" subtype="single">
<title>使用rewrites的配置文件</title>
<filename>vercel.json</filename>
<yb_code lang="json">{
"version": 2,
"builds": [
{
"src": "index.html",
"use": "@vercel/static"
}
],
"rewrites": [
{
"source": "/(.*)",
"destination": "/index.html"
}
]
}</yb_code>
</yb_canvas>
